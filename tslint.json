{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": false,
    "max-classes-per-file": false,
    "max-line-length": {
      "options": [
        80
      ]
    },
    "member-access": [
      true,
      "no-public"
    ],
    "no-console": false,
    "no-empty": false,
    "no-empty-interface": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "prefer-const": true,
    "trailing-comma": false
  }
}